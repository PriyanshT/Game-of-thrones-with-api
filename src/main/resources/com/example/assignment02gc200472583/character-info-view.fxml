         <bottom>
            <Button mnemonicParsing="false" onAction="#goBack" prefHeight="34.0" prefWidth="74.0" text="Go Back" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets bottom="20.0" />
               </BorderPane.margin>
            </Button>
         </bottom>
         <center>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="10.0" BorderPane.alignment="CENTER">
               <children>
                  <ImageView fx:id="imageView" fitHeight="135.0" fitWidth="169.0" pickOnBounds="true" preserveRatio="true" />
                  <GridPane prefHeight="200.0" prefWidth="600.0">
                    <columnConstraints>
                      <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
